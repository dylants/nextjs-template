{
  "name": "nextjs-template",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "20.18.0",
    "npm": "10.8.2"
  },
  "scripts": {
    "build": "next build",
    "check-types": "tsc --noemit",
    "dev": "next dev",
    "eslint": "next lint",
    "lint": "npm run eslint && npm run check-types",
    "start": "next start",
    "test": "dotenv -c test -- jest",
    "test:coverage": "dotenv -c test -- jest --config ./jest.all.config.ts --coverage --runInBand",
    "test:watch": "npm run test -- --watch"
  },
  "dependencies": {
    "bcrypt": "5.1.1",
    "next": "14.2.18",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@types/bcrypt": "5.0.2",
    "@types/jest": "29.5.14",
    "@types/node": "20.14.8",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "@typescript-eslint/eslint-plugin": "8.15.0",
    "@typescript-eslint/parser": "8.15.0",
    "dotenv-cli": "7.4.3",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.18",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.2.1",
    "jest": "29.7.0",
    "postcss": "8.4.49",
    "prettier": "3.3.3",
    "tailwindcss": "3.4.14",
    "ts-node": "10.9.2",
    "typescript": "5.6.3"
  }
}
